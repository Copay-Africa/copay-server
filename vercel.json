{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "\\/(.*)",
            "dest": "api/index.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "HEAD",
                "OPTIONS"
            ]
        }
    ],
    "functions": {
        "api/index.ts": {
            "maxDuration": 60
        }
    },
    "env": {
        "NODE_ENV": "production"
    },
    "installCommand": "npm install",
    "buildCommand": "npm run vercel-build"
}