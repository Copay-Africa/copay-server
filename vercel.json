{
    "version": 2,
    "routes": [
        {
            "src": "\\/(.*)",
            "dest": "dist/api/index.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "HEAD",
                "OPTIONS"
            ]
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "dist/api/index.js": {
            "maxDuration": 60
        }
    },
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "buildCommand": "npm run vercel-build"
}